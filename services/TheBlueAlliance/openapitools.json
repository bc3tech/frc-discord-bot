{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.11.0"
  },
  "additionalProperties": {
    "modelPropertySorting": "alphabetical",
    "netCoreProjectFile": true,
    "nonPublicApi": false,
    "nullableReferenceTypes": true,
    "operationParameterSorting": "alphabetical",
    "packageName": "TheBlueAlliance",
    "returnICollection": true,
    "targetFramework": "net8.0",
    "useCollection": true,
    "useDateTimeOffset": true,
    "useCPM": true,
    "useGenericHost": true,
    "validatable": false,
    "reservedWords": "in,out,ref,params,async,await",
    "supportsFileParameters": false
  },
  "files": {
    "Directory.Packages.props.mustache": {
      "destinationFilename": "Directory.Packages.props",
      "templateType": "SupportingFiles"
    },
    "AssemblyInfo.mustache": {
      "destinationFilename": "src/TheBlueAlliance/Properties/AssemblyInfo.cs",
      "templateType": "SupportingFiles"
    }
  }
}